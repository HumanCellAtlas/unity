<% if params[:target].blank? %>
  $('#reference-wdl-content-target').html("<%= escape_javascript(render partial: '/site/wdl_content') %>");

  $('#wdl-content-modal').modal('show');

  $('#get-reference-wdl').removeClass('disabled');
<% else %>
  closeModalSpinner('#generic-modal-spinner', '#generic-modal', function() {
      $('#<%= params[:target] %>').val("<%= escape_javascript(@pipeline_wdl.html_safe) %>");
      $('#<%= params[:target] %>').data({hasWdl: true})
  });
<% end %>